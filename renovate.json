{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    "config:base"
  ],
  "baseBranches": [
    "main",
    "develop"
  ],
  "packageRules": [
    {
      "matchBaseBranches": ["main"],
      "matchFiles": ["./HasDependenciesProject/Package.swift"],
      "matchManagers": ["swift"]
    },
    {
      "matchBaseBranches": ["develop"],
      "matchFiles": ["./Project2/Package.swift"],
      "matchManagers": ["swift"]
    }
  ]
}
