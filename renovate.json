{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    "config:base"
  ],
  "baseBranches": [
    "main",
    "develop"
  ],
  "packageRules": [
    {
      "matchFiles": ["./HasDependenciesProject/Package.swift"],
      "matchManagers": ["swift"],
      "baseBranches": ["main"]
    },
    {
      "matchFiles": ["./Project2/Package.swift"],
      "matchManagers": ["swift"],
      "baseBranches": ["develop"]
    }
  ]
}
